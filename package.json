{
  "private": true,
  "type": "module",
  "name": "autority-api-challenge",
  "version": "1.0.0",
  "description": "Boilerplate for api to Autority challenge",
  "scripts": {
    "build": "babel src --out-dir dist",
    "start": "node ./dist/bin/www.js",
    "dev": "nodemon --ignore dist/ --ignore test/ --exec babel-node ./src/bin/www.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "mocha src/**/*.test.js",
    "test-coverage": "c8 --reporter=html --reporter=text mocha 'src/**/*.test.js' ",
    "migrate": "npx sequelize-cli db:migrate"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-validator": "^6.4.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "nodemailer": "^6.4.16",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.20.1",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.23.7",
    "@types/sinon": "^17.0.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^7.2.0",
    "mocha-tap-reporter": "^0.1.3",
    "nodemon": "^2.0.3",
    "nyc": "15.1.0",
    "proxyquire": "^2.1.3",
    "sequelize-cli": "^6.4.1",
    "sinon": "^17.0.1"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  }
}
